<?xml version='1.0' encoding='utf-8'?>
<widget id="com.p5.p5Art" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>P5Art</name>
    <description>P5Art for p5.js at mobile device</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/img/192x192.png" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
